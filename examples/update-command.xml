<?xml version="1.0" encoding="utf-8" standalone="no"?>
<epp xmlns="urn:ietf:params:xml:ns:epp-1.0">
  <command>
    <update>
      <domain:update
        xmlns:domain="urn:ietf:params:xml:ns:domain-1.0">
        <domain:name>example.com</domain:name>
      </domain:update>
    </update>
    <extension>
      <auxcontact:update
        xmlns:auxcontact="urn:ietf:params:xml:ns:auxcontact-0.1">
        <auxcontact:rem>
          <auxcontact:contact
            type="dns-operator">ClientZ</auxcontact:contact>
        </auxcontact:rem>
        <auxcontact:add>
          <auxcontact:contact
            type="dns-operator">ClientXYZ</auxcontact:contact>
        </auxcontact:add>
      </auxcontact:update>
    </extension>
    <clTRID>ABC-12345</clTRID>
  </command>
</epp>
